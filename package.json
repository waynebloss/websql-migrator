{
  "name": "websql-migrator",
  "version": "1.0.0",
  "description": "A Web SQL migrator utility.",
  "homepage": "https://github.com/waynebloss/websql-migrator#readme",
  "repository": "https://github.com/waynebloss/websql-migrator",
  "bugs": "https://github.com/waynebloss/websql-migrator/issues",
  "author": "Wayne Bloss <waynebloss@gmail.com>",
  "license": "MIT",
  "private": false,
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "LICENSE.md",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "patch-build": "run-s version-patch build",
    "minor-build": "run-s version-minor build",
    "major-build": "run-s version-major build",
    "build": "run-s clean build-js copy-defs",
    "build-js": "tsc",
    "clean": "shx rm -rf \"lib/*\"",
    "copy-defs": "copyfiles -u 1 \"src/**/*.d.ts\" lib",
    "test": "echo \"Error: no test specified\" && exit 1",
    "version-major": "npm version major --no-git-tag-version",
    "version-minor": "npm version minor --no-git-tag-version",
    "version-patch": "npm version patch --no-git-tag-version"
  },
  "dependencies": {
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/websql": "^0.0.27",
    "copyfiles": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.2",
    "typescript": "^3.2.4"
  },
  "keywords": [
    "migrate",
    "migration",
    "sql",
    "sqlite",
    "websql",
    "web"
  ]
}
